<div class="card">
  <%= link_to image_tag(review.image, class: :card__img), party_path(review.party.id) %>
  <div class="card__body">
    <%= link_to review.title, party_path(review.id), class: :card__title%>
    <p class="card__summary">
      <%= link_to review.content %>
    </p>
    <%= link_to "by #{review.user.nickname}", "/users/#{review.user.id}", class: :card__user %>
  </div>
</div>

